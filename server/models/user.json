{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "scope": {
    "include": [
      "positions"
    ]
  },
  "properties": {
    "fullName": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "gender": {
      "type": "number"
    },
    "avatar": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "positions": {
      "type": "hasMany",
      "model": "Position",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {

  }
}
